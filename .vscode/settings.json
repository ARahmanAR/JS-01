{
	"workbench.sideBar.location": "right",
	"editor.fontSize": 19,
	"editor.fontFamily": "'Cascadia Code NF', 'Cascadia Code', Menlo, Consolas, 'Liberation Mono', monospace",
	"editor.fontLigatures": true,
	"editor.wordWrap": "on",
	"files.autoSave": "afterDelay",
	"workbench.colorTheme": "Aura Dark",
	"terminal.integrated.fontFamily": "'Cascadia Code NF', 'Cascadia Code', Menlo, Consolas, 'Liberation Mono', monospace",
	"debug.console.fontFamily": "'Cascadia Code NF', 'Cascadia Code', Menlo, Consolas, 'Liberation Mono', monospace",
	"terminal.integrated.gpuAcceleration": "auto",
	"editor.cursorBlinking": "expand",
	"editor.cursorStyle": "block",
	"editor.cursorWidth": 1,
	"workbench.colorCustomizations": {
		"editorCursor.background": "#00000000",
        "editorCursor.foreground": "#90EE90"
	},
	"terminal.integrated.fontSize": 18,
	"editor.mouseWheelZoom": true,
	"terminal.integrated.mouseWheelZoom": true,
	"terminal.integrated.fontLigatures.enabled": true,
	"code-runner.runInTerminal": true,
	"git.autofetch": true,
    "editor.lineHeight": 3,
    "editor.fontWeight": "600",
    "terminal.integrated.fontWeight": "600"
}
